{
  "name": "from-package",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rescripts/cli": "^0.1.0",
    "@rescripts/rescript-use-babel-config": "^0.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    [
      "use-babel-config",
      {
        "presets": [
          "react-app"
        ],
        "plugins": [
          [
            "module-resolver",
            {
              "root": ".",
              "alias": {
                "~": "./src"
              }
            }
          ]
        ]
      }
    ]
  ]
}
